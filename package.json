{
  "name": "nestjs-be",
  "version": "0.0.1",
  "description": "NestJS Backend",
  "author": "Ankit Kumar Pandit",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:migrate:dev": "npm run prisma:dev:deploy && npm run start:dev",
    "start:migrate:prod": "npm run prisma:dev:deploy && npm run start:prod",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:fix": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts --fix",
    "lint:file": "eslint --ignore-path .gitignore .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:dev:deploy": "npx prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=.prisma/schema.prisma --create-only",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/microservices": "^10.4.9",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.2.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.52.0",
    "@opentelemetry/sdk-node": "^0.54.1",
    "@prisma/client": "^5.21.1",
    "@prisma/instrumentation": "^5.22.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "axios": "^1.7.7",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express-handlebars": "^8.0.1",
    "google-auth-library": "^9.14.2",
    "hbs": "^4.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "nestjs-otel": "^6.1.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.6.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/randomstring": "^1.3.0",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "add": "^2.0.6",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "node-mocks-http": "^1.16.1",
    "pg": "^8.11.5",
    "prettier": "^3.0.0",
    "prisma": "^5.21.1",
    "sinon": "^19.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "yarn": "^1.22.22"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
